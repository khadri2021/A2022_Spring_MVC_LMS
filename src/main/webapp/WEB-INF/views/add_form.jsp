<html>
<body bgcolor="#d1ebd6">
<center>
	<h1>SBI BANK LOAN APPLICATION</h1>
	<hr>
</center>
	<form action="add/register" method="post">
		<table border="4">
			<tr>
				<td colspan="2" align="center">ADD BANK  ACCOUNT FORM</td>
			</tr>
			<tr>
				<td>FIRST NAME :</td>
				<td><input type="text" name="fName" required="required"></td>
			</tr>
			</tr>
			<tr>
				<td>LAST NAME :</td>
				<td><input type="text" name="lName"></td>
			</tr>
			<tr>
				<td>FATHER/GUARDIAN NAME :</td>
				<td><input type="text" name="Name" required="required"></td>
			</tr>
			<tr>
				<td>DOB :</td>
				<td><input type="text" name="sAge"></td>
			</tr>
			</tr>
			<tr>
				<td>EMAIL:</td>
				<td><input type="text" name="email"></td>
			</tr>
			</tr>
			<tr>
				<td>PHONE NUMBER:</td>
				<td><input type="text" name="phone" required="required"></td>
			</tr>
			</tr>
			<tr>
				<td>ALTERNATIVE NUMBER :</td>
				<td><input type="text" name="alterPhone"></td>
			</tr>
			<tr>
				<td>ACCOUNT TYPE:</td>
				<td><input type="radio" name="type" value="savings">
					SAVINGS <input type="radio" name="type" value="business">BUSINESS
				
			</tr>
			 

			<tr>
				<td>CURRENT ADDRESS :</td>
				<td><textarea rows="6" cols="30" name="cAddress">
				
				</textarea></td>
			</tr>
			<tr>
				<td>PERMANENT ADDRESS :</td>
				<td><textarea rows="6" cols="30" name="pAddress" required="required">
				
				</textarea></td>
			</tr>
			
			</tr>
			<tr>
				<td>ADHAAR NO:</td>
				<td><input type="text" name="adhaar" required="required"></td>
			</tr>
			 
			<tr>
				<td>PAN NO:</td>
				<td><input type="text" name="pan" required="required"></td>
			</tr>
			</tr>
			<tr>
				<td>NOMINEE NAME :</td>
				<td><input type="text" name="nName" required="required"></td>
			</tr>
			 
			<tr>
				<td>NOMINEE ADHAARNO :</td>
				<td><input type="text" name="ano" required="required"></td>
			</tr>
			 
			<tr>
				<td>NOMINEE ACCOUNTNO :</td>
				<td><input type="text" name="aco"></td>
			</tr>
		 	 
			<tr>
				<td>CIBIL SCORE :</td>
				<td><input type="text" name="cibil"></td>
			</tr>
			 
				<td colspan="2" align="center"><input type="submit"
					name="sSubmit" value="Register"></td>
			</tr>

		</table>
		 		<table bgcolor="#99ff99">
			<tr>

				<td colspan="2" align="center"><a href="../../adminpage">Go
						To</a> previous page</td>
			</tr>
		</table>
		
		
	</form>

</body>
</html>
